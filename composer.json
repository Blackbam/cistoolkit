{
  "name": "blackbam/cistoolkit",
  "type": "library",
  "description": "A generic toolkit containing a lot of very useful PHP classes",
  "keywords": [
    "templating",
    "debugging",
    "math",
    "url"
  ],
  "homepage": "https://www.blackbam.at/",
  "license": "MIT",
  "authors": [
    {
      "name": "David St\u00f6ckl",
      "email": "admin@blackbam.at",
      "homepage": "https://www.blackbam.at/"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-mbstring": "*",
    "ext-spl": "*",
    "ext-openssl": "*",
    "ext-simplexml": "*",
    "ext-ftp": "*",
    "ext-ssh2": "*"
  },
  "autoload": {
    "psr-4": {
      "CisTools\\": "src/"
    }
  },
  "require-dev": {
    "codeception/codeception": "^5.3.1",
    "codeception/module-phpbrowser": "^3.0.1",
    "codeception/module-asserts": "^3.2.0"
  }
}
